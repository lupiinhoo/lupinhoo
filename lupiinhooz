local Context = game:GetService("ContextActionService")

local TweenService = game:GetService("TweenService")
local TS = TweenInfo.new(
	0.7,
	Enum.EasingStyle.Quint
)

local function MenuHandler(_, state)
	if state == Enum.UserInputState.Begin then
		Frame.Visible = not Frame.Visible
		TweenService:Create(Frame,TS,{
			Position = Frame.Visible and UDim2.new(0.243273526, 0, 0.171668425, 0) or UDim2.new(-0.243273526, 0, 0.171668425, 0)
		}):Play()
	end
end

Context:BindAction("MenuHandler", MenuHandler, false, Enum.KeyCode.M)
