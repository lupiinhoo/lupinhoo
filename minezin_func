, 180, 0, 190), UDim2.new(0, 150, 0, 40)) local function a(c) if not c.Character then return end local dm = c.Character:FindFirstChild("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116") local nd = d.Character if not dm or not nd then return end local gn = nd:FindFirstChild("\71\108\111\99\107") if not gn then return end local hg = { gn, { ["\112"] = Vector3.new(-1934.757568359375, -73.66830444335938, -11445.2548828125), ["\112\105\100"] = 1, ["\112\97\114\116"] = dm, ["\100"] = 0.2078668624162674, ["\109\97\120\68\105\115\116"] = 0.2078594863414764, ["\104"] = c.Character.Humanoid, ["\109"] = Enum.Material.Plastic, ["\115\105\100"] = 1, ["\116"] = 0.011482002849878449, ["\110"] = Vector3.new(-0.9999971389770508, -0.0006082971813157201, 0.002314852550625801) } } m.WeaponsSystem.Network.WeaponHit:FireServer(unpack(hg)) end aq.MouseButton1Click:Connect(function() local io = sr.Text local ji = k:FindFirstChild(io) if ji and ji ~= d then a(ji) end end) local fp = false ca.MouseButton1Click:Connect(function() fp = not fp ca.Text = "\75\105\108\108\32\80\108\97\121\101\114\32\78\97\109\101\32\76\111\111\112\58\32" .. (fp and "\79\78" or "\79\70\70") end) kc.MouseButton1Click:Connect(function() for _, plr in pairs(k:GetPlayers()) do if plr ~= d then a(plr) end end end) local of = false mk.MouseButton1Click:Connect(function() of = not of mk.Text = "\75\105\108\108\32\65\108\108\32\76\111\111\112\58\32" .. (of and "\79\78" or "\79\70\70") end) spawn(function() while true do if fp then local io = sr.Text local ji = k:FindFirstChild(io) if ji and ji ~= d then a(ji) end end if of then for _, plr in pairs(k:GetPlayers()) do if plr ~= d then a(plr) end end end task.wait(0.1) end end) 
