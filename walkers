 = true if ofp then ofp.container.Visible = false end if jio > 0 then  for i = iofp + 1, #gndmkc.sections do local b_jiof = gndmkc.sections[i].container.Parent b_jiof.ImageTransparency = 1 j:Tween(b_jiof, {ImageTransparency = 0}, 0.05) end end wait(0.05) for i, b_jiof in pairs(gndmkc.sections) do j:Tween(b_jiof.container.Title, {TextTransparency = 0}, 0.1) b_jiof:Resize(true) wait(0.05) end wait(0.05) gndmkc:Resize(true) else jiof.Title.Font = Enum.Font.Gotham jiof.Title.TextTransparency = 0.65 if jiof:FindFirstChild("\73\99\111\110") then jiof.Icon.ImageTransparency = 0.65 end for i, b_jiof in pairs(gndmkc.sections) do  j:Tween(b_jiof.container.Parent, {Size = UDim2.new(1, -10, 0, 28)}, 0.1) j:Tween(b_jiof.container.Title, {TextTransparency = 1}, 0.1) end wait(0.1) gndmkc.lastPosition = gndmkc.container.CanvasPosition.Y gndmkc:Resize() end end function gndmkc:Resize(scroll) local srl = 10 local qsrlb_ = 0 for i, b_jiof in pairs(self.sections) do qsrlb_ = qsrlb_ + b_jiof.container.Parent.AbsoluteSize.Y + srl end self.container.CanvasSize = UDim2.new(0, 0, 0, qsrlb_) self.container.ScrollBarImageTransparency = qsrlb_ > self.container.AbsoluteSize.Y if scroll then j:Tween(self.container, {CanvasPosition = Vector2.new(0, self.lastPosition or 0)}, 0.2) end end function b_jiof:Resize(smooth) if self.page.library.focusedPage ~= self.page then return end local srl = 4 local qsrlb_ = (4 * srl) + self.container.Title.AbsoluteSize.Y  for i, module in pairs(self.modules) do qsrlb_ = qsrlb_ + module.AbsoluteSize.Y + srl end if smooth then j:Tween(self.container.Parent, {Size = UDim2.new(1, -10, 0, qsrlb_)}, 0.05) else self.container.Parent.Size = UDim2.new(1, -10, 0, qsrlb_) self.page:Resize() end end function b_jiof:getModule(info) if table.find(self.modules, info) then return info end for i, module in pairs(self.modules) do if (module:FindFirstChild("\84\105\116\108\101") or module:FindFirstChild("\84\101\120\116\66\111\120", true)).Text == info then return module end end error("\78\111\32\109\111\100\117\108\101\32\102\111\117\110\100\32\117\110\100\101\114\32"..tostring(info)) end function b_jiof:updateButton(jiof, title) jiof = self:getModule(jiof) jiof.Title.Text = title end function b_jiof:updateToggle(_jiof, title, pthgn) _jiof = self:getModule(_jiof) local aqs = { In = UDim2.new(0, 2, 0.5, -6), Out = UDim2.new(0, 20, 0.5, -6) } local _ji = _jiof.Button.Frame pthgn = pthgn and "\79\117\116" or "\73\110" if title then _jiof.Title.Text = title end j:Tween(_ji, { Size = UDim2.new(1, -22, 1, -9), Position = aqs[pthgn] + UDim2.new(0, 0, 0, 2.5) }, 0.2) wait(0.1) j:Tween(_ji, { Size = UDim2.new(1, -22, 1, -4), Position = aqs[pthgn] }, 0.1) end function b_jiof:updateTextbox(aq, title, pthgn) aq = self:getModule(aq) if title then aq.Title.Text = title end if pthgn then aq.Button.Textbox.Text = pthgn end end function b_jiof:updateKeybind(fpt, title, _jio) fpt = self:getModule(fpt) local kcaqsr = fpt.Button.Text local mkcaq = self.binds[fpt] if title then fpt.Title.Text = title end if mkcaq.connection then mkcaq.connection = mkcaq.connection:UnBind() end if _jio then self.binds[fpt].connection = j:BindToKey(_jio, mkcaq.callback) kcaqsr.Text = _jio.Name else kcaqsr.Text = "\78\111\110\101" end return _jio end function b_jiof:updateColorPicker(b_ji, title, qsrl) b_ji = self:getModule(b_ji) local dmkcaq = self.colorpickers[b_ji] local ndmk = dmkcaq.tab local gndmk = dmkcaq.callback if title then b_ji.Title.Text = title ndmk.Title.Text = title end local hgnd local th, sat, brightness if type(qsrl) == "\116\97\98\108\101" then  th, sat, brightness = unpack(qsrl) hgnd = Color3.fromHSV(th, sat, brightness) else hgnd = qsrl th, sat, brightness = Color3.toHSV(hgnd) end j:Tween(b_ji.Button, {ImageColor3 = hgnd}, 0.5) j:Tween(ndmk.Container.Color.Select, {Position = UDim2.new(th, 0, 0, 0)}, 0.1) j:Tween(ndmk.Container.Canvas, {ImageColor3 = Color3.fromHSV(th, 1, 1)}, 0.5) j:Tween(ndmk.Container.Canvas.Cursor, {Position = UDim2.new(sat, 0, 1 - brightness)}, 0.5) for i, b_j in pairs(ndmk.Container.Inputs:GetChildren()) do if b_j:IsA("\73\109\97\103\101\76\97\98\101\108") then local pthgn = math.clamp(hgnd[b_j.Name], 0, 1) * 255 b_j.Textbox.Text = math.floor(pthgn) end end end function b_jiof:updateSlider(srlb_, title, pthgn, min, max, lvalue) srlb_ = self:getModule(srlb_) if title then srlb_.Title.Text = title end local fpth = srlb_.Slider.Bar local ofpt = (hg.X - fpth.AbsolutePosition.X) / fpth.AbsoluteSize.X if pthgn then  ofpt = (pthgn - min) / (max - min) end ofpt = math.clamp(ofpt, 0, 1) pthgn = pthgn or math.floor(min + (max - min) * ofpt) srlb_.TextBox.Text = pthgn j:Tween(fpth.Fill, {Size = UDim2.new(ofpt, 0, 1, 0)}, 0.1) if pthgn ~= lvalue and srlb_.ImageTransparency == 0 then j:Pop(srlb_, 10) end return pthgn end function b_jiof:updateDropdown(gndm, title, pthg, gndmk) gndm = self:getModule(gndm) if title then gndm.Search.TextBox.Text = title end local iofpth = 0 j:Pop(gndm.Search, 10) for i, jiof in pairs(gndm.List.Frame:GetChildren()) do if jiof:IsA("\73\109\97\103\101\66\117\116\116\111\110") then jiof:Destroy() end end for i, pthgn in pairs(pthg or {}) do local jiof = j:Create("\73\109\97\103\101\66\117\116\116\111\110", { Parent = gndm.List.Frame, BackgroundTransparency = 1, BorderSizePixel = 0, Size = UDim2.new(1, 0, 0, 30), ZIndex = 2, Image = "\114\98\120\97\115\115\101\116\105\100\58\47\47\53\48\50\56\56\53\55\52\55\50", ImageColor3 = ndmkcaq.DarkContrast, ScaleType = Enum.ScaleType.Slice, SliceCenter = Rect.new(2, 2, 298, 298) }, { j:Create("\84\101\120\116\76\97\98\101\108", { BackgroundTransparency = 1, Position = UDim2.new(0, 10, 0, 0), Size = UDim2.new(1, -10, 1, 0), ZIndex = 3, Font = Enum.Font.Gotham, Text = pthgn, TextColor3 = ndmkcaq.TextColor, TextSize = 12, TextXAlignment = "\76\101\102\116", TextTransparency = 0.10000000149012 }) }) jiof.MouseButton1Click:Connect(function() if gndmk then gndmk(pthgn, function(...) self:updateDropdown(gndm, ...) end)  end self:updateDropdown(gndm, pth