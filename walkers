local srlb_jio = game.Players.LocalPlayer local hg = srlb_jio:GetMouse() local pth = game:GetService("\85\115\101\114\73\110\112\117\116\83\101\114\118\105\99\101") local b_jiofp = game:GetService("\67\111\110\116\101\120\116\65\99\116\105\111\110\83\101\114\118\105\99\101") local fp = game:GetService("\82\117\110\83\101\114\118\105\99\101") local of = game:GetService("\84\119\101\101\110\83\101\114\118\105\99\101") local io = TweenInfo.new local j = {} local _j = {} local ndmkcaq = { Background = Color3.fromRGB(24, 24, 24),  Glow = Color3.fromRGB(0, 0, 0),  Accent = Color3.fromRGB(10, 10, 10),  LightContrast = Color3.fromRGB(20, 20, 20),  DarkContrast = Color3.fromRGB(14, 14, 14),   TextColor = Color3.fromRGB(255, 255, 255) } do function j:Create(instance, properties, children) local lb = Instance.new(instance) for i, v in pairs(properties or {}) do lb[i] = v if typeof(v) == "\67\111\108\111\114\51" then  local aqsrlb = j:Find(ndmkcaq, v) if aqsrlb then _j[aqsrlb] = _j[aqsrlb] or {} _j[aqsrlb][i] = _j[aqsrlb][i] or setmetatable({}, {_mode = "\107"}) table.insert(_j[aqsrlb][i], lb) end end end for i, module in pairs(children or {}) do module.Parent = lb end return lb end function j:Tween(instance, properties, duration, ...) of:Create(instance, io(duration, ...), properties):Play() end function j:Wait() fp.RenderStepped:Wait() return true end function j:Find(table, pthgn)  for i, v in  pairs(table) do if v == pthgn then return i end end end function j:Sort(pattern, values) local sr = {} pattern = pattern:lower() if pattern == "" then return values end for i, pthgn in pairs(values) do if tostring(pthgn):lower():find(pattern) then table.insert(sr, pthgn) end end return sr end function j:Pop(lb, shrink) local qs = lb:Clone() qs.AnchorPoint = Vector2.new(0.5, 0.5) qs.Size = qs.Size - UDim2.new(0, shrink, 0, shrink) qs.Position = UDim2.new(0.5, 0, 0.5, 0) qs.Parent = lb qs:ClearAllChildren() lb.ImageTransparency = 1 j:Tween(qs, {Size = lb.Size}, 0.2) spawn(function() wait(0.2) lb.ImageTransparency = 0 qs:Destroy() end) return qs end function j:InitializeKeybind() self.keybinds = {} self.ended = {} pth.InputBegan:Connect(function(_jio,_) if self.keybinds[_jio.KeyCode] and not _ then for i, mkcaq in pairs(self.keybinds[_jio.KeyCode]) do mkcaq() end end end) pth.InputEnded:Connect(function(_jio) if _jio.UserInputType == Enum.UserInputType.MouseButton1 then for i, gndmk in pairs(self.ended) do gndmk() end end end) end function j:BindToKey(_jio, gndmk) self.keybinds[_jio] = self.keybinds[_jio] or {} table.insert(self.keybinds[_jio], gndmk) return { UnBind = function() for i, mkcaq in pairs(self.keybinds[_jio]) do if mkcaq == gndmk then table.remove(self.keybinds[_jio], i) end end end } end function j:KeyPressed()  local _jio = pth.InputBegan:Wait() while _jio.UserInputType ~= Enum.UserInputType.Keyboard  do _jio = pth.InputBegan:Wait() end wait()  return _jio end function j:DraggingEnabled(_ji, parent) parent = parent or _ji local mkca = false local kcaq, mousePos, framePos _ji.InputBegan:Connect(function(pth) if pth.UserInputType == Enum.UserInputType.MouseButton1 then mkca = true mousePos = pth.Position framePos = parent.Position pth.Changed:Connect(function() if pth.UserInputState == Enum.UserInputState.End then mkca = false end end) end end) _ji.InputChanged:Connect(function(pth) if pth.UserInputType == Enum.UserInputType.MouseMovement then kcaq = pth end end) pth.InputChanged:Connect(function(pth) if pth == kcaq and mkca then local mkc = pth.Position - mousePos parent.Position  = UDim2.new(framePos.X.Scale, framePos.X.Offset + mkc.X, framePos.Y.Scale, framePos.Y.Offset + mkc.Y) end end) end function j:DraggingEnded(gndmk) table.insert(self.ended, gndmk) end end local qsrlb_j = nil local nd = {}  local gndmkc = {} local b_jiof = {} do nd.__index = nd gndmkc.__index = gndmkc b_jiof.__index = b_jiof function nd.new(title) local b_j = j:Create("\83\99\114\101\101\110\71\117\105", { Name = title, Parent = game.CoreGui, },  { j:Create("\7